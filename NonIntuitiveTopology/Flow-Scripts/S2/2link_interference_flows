#High Priority
ovs-ofctl add-flow ashish in_port=2,dl_dst=b8:27:eb:8b:02:7e,actions=set_queue:7,output:4
#Low Priority
ovs-ofctl add-flow ashish in_port=2,dl_dst=b8:27:eb:6b:f7:38,actions=set_queue:0,output:4
#Medium Priority
ovs-ofctl add-flow ashish in_port=2,dl_dst=28:f1:0e:1d:10:a4,actions=set_queue:4,output:4
#Return flows are straightforward
ovs-ofctl add-flow ashish in_port=4,actions=output:2
