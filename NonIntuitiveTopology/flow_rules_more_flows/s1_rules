#Flows to h4 originating from S1(8,10,12)
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.8,nw_dst=192.168.1.40,udp,tp_dst=10000,actions=set_queue:1,output:7
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.10,nw_dst=192.168.1.40,udp,tp_dst=10001,actions=set_queue:4,output:7
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.12,nw_dst=192.168.1.40,udp,tp_dst=10002,actions=set_queue:7,output:7

#Flows to h2 originating from S1(9,10,11)
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.9,nw_dst=192.168.1.20,udp,tp_dst=10003,actions=set_queue:1,output:7
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.10,nw_dst=192.168.1.20,udp,tp_dst=10004,actions=set_queue:4,output:7
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.11,nw_dst=192.168.1.20,udp,tp_dst=10005,actions=set_queue:7,output:7

#Flows to h2 originating from S3(29,30,31)
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.29,nw_dst=192.168.1.20,udp,tp_dst=10006,actions=set_queue:1,output:7
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.30,nw_dst=192.168.1.20,udp,tp_dst=10007,actions=set_queue:4,output:7
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.31,nw_dst=192.168.1.20,udp,tp_dst=10008,actions=set_queue:7,output:7

#Flow from h5 to h4
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.15,nw_dst=192.168.1.40,udp,tp_dst=10009,actions=output:8
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.15,nw_dst=192.168.1.40,udp,tp_dst=10010,actions=output:7

#ping traffic
#ping between 8 and 40
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.8,nw_dst=192.168.1.40,actions=output:7
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.40,nw_dst=192.168.1.8,actions=output:1
#ping between 10 and 40
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.10,nw_dst=192.168.1.40,actions=output:7
ovs-ofctl add-flow br0 ip,nw_src=192.168.1.40,nw_dst=192.168.1.10,actions=output:2
